{
    "manifest_version": 3,
    "name": "URL Phishing Checker",
    "version": "1.0",
    "description": "Check if the current URL is a phishing site using a Random Forest model please!",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png"
      }
    }
  }
  